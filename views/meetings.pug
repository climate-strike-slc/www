extends layout
block page
	- var menu = 'meetings'
block content
	script(type="text/javascript", src="/scripts/basefunctions.js?v="+Math.random())

	div#vue
		div.wrapper.nav
			include partials/menu.pug
		div.wrapper.main
			div.container
				//- @mouseover="activate(k)"
				//- @mouseleave="deactivate(k)"
				a.ui.img(
					v-if="data && data !== '' && data.meetings.length > 0"
					v-for="(k, i) in keys"
					@touchstart="activate(k)"
					@click="dialog(k, i)"
					@touchend="dialog(k, i)"
					:class="k === active ? 'active' : ''"
					:id="k"
				)
					//- embed(v-if="data.meetings" :src="data.meetings[i].URL")
					p(v-if="data.meetings" v-text="data.meetings[i].topic")

	include vue/meetings.vue.pug
